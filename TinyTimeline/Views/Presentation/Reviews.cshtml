@using TinyTimeline.Models
@model ReviewsModel

@{
    Layout = "Layout";
}

<div class="margin_top">
    @await Html.PartialAsync("Menu", new MenuModel {SessionId = Model.SessionInfo.SessionId})
    @Html.DisplayFor(m => m.SessionInfo)
    @foreach (var review in Model.Reviews)
    {
        @Html.DisplayFor(m => review)
    }

    @Html.ActionLink("Add review", "AddReview", "Interaction", new {sessionId = Model.SessionInfo.SessionId}, new {@class = "btn btn-primary btn-sm"})
</div>